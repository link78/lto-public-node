{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "lto-data",
      "host": {
        "sourcePath": "/lto/data"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "public-node",
      "image": "ltonetwork/public-node",
      "essential": true,
      "memoryReservation": 1024,
      "mountPoints": [
        {
          "sourceVolume": "lto-data",
          "containerPath": "/lto/data"
        }
      ]
    }
  ]
}
